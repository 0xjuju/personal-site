{
  "name": "adaptify-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run lint:watch\" \"pnpm run type-check:watch\" \"next dev\"",
    "lint": "next lint",
    "lint:watch": "chokidar \"src/**/*.{ts,tsx}\" -c \"eslint src --ext .ts,.tsx\"",
    "type-check:watch": "tsc --noEmit --watch",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@next/font": "^14.2.15",
    "clsx": "^2.1.1",
    "framer-motion": "^12.12.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.515.0",
    "next": "15.1.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-tooltip": "^5.29.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.1.8",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5"
  }
}
